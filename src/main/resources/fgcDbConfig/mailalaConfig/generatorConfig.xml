        <!--serviceImp实现类文件生成路径-->
        <property name="diy_serviceImpl_targetPackage" value="${diy_serviceImpl_targetPackage}"/>
        <property name="diy_serviceImpl_targetProject" value="${diy_serviceImpl_targetProject}"/>

